<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.gc.expressOL">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.gc.expressOL.activity.WelcomeActivity"
            android:exported="true"
            android:theme="@style/AppWelcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.gc.expressOL.activity.MainActivity"
            android:label="MainActivity" />

        <activity
            android:name="com.gc.expressOL.activity.ReceiveOrderActivity"
            android:label="Receive_orderActivity" />
        <activity
            android:name="com.gc.expressOL.activity.SendActivity"
            android:label="SendActivity" />
        <activity
            android:name="com.gc.expressOL.activity.UsersActivity"
            android:label="@string/title_activity_users" />
        <activity
            android:name="com.gc.expressOL.activity.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name="com.gc.expressOL.activity.RegisterActivity"
            android:label="@string/title_activity_register" />
        <activity
            android:name="com.gc.expressOL.activity.RetrievePwdActivity"
            android:label="@string/title_activity_retrieve_pwd" />
        <activity
            android:name="com.gc.expressOL.activity.SetNewPassWordActivity"
            android:label="@string/title_activity_set_new_password" />
        <activity
            android:name="com.gc.expressOL.activity.InquireActivity"
            android:label="@string/title_activity_inquire" />
        <activity
            android:name="com.gc.expressOL.activity.OrderDetailsActivity"
            android:label="@string/title_activity_check" />
        <activity
            android:name="com.gc.expressOL.activity.AboutAuthorActivity"
            android:label="@string/title_activity_about_author" />


    </application>

</manifest>